<?xml version="1.0" encoding="utf-8"?>
<!-- In Progress Status Badge - Blue Gradient -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:angle="135"
        android:startColor="#3B82F6"
        android:endColor="#60A5FA"
        android:type="linear" />
    <corners android:radius="20dp" />
    <padding
        android:left="12dp"
        android:right="12dp"
        android:top="6dp"
        android:bottom="6dp" />
</shape>
